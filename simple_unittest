import unittest

def additional(a, b):
    return a + b

def subtraction(a, b):
    return a - b

class Test_additional(unittest.TestCase):
    def test_additional(self):
        test_list = [2, 4]
        expected = 6
        actual = additional(test_list[0], test_list[1])
        self.assertEqual(expected, actual)

